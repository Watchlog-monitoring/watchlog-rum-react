{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "jsx": "react-jsx",
    "lib": [
      "ES2019",
      "DOM",
      "DOM.Iterable"
    ],
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "declaration": true,
    "resolveJsonModule": true,
    "noEmit": false
  },
  "include": [
    "src"
  ]
}